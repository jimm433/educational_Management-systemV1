# ===== .env.production (生產環境專用) =====
# 生產環境配置
NODE_ENV=production
DEBUG=false
LOG_LEVEL=error

# Firebase 生產環境設定
FIREBASE_USE_EMULATOR=false

# 生產 API 端點
VITE_API_BASE_URL=https://classhelper-aa6be.web.app
VITE_FUNCTIONS_BASE_URL=https://us-central1-classhelper-aa6be.cloudfunctions.net

# 生產環境安全設定
ENABLE_DEV_TOOLS=false
ENABLE_MOCK_DATA=false
SKIP_AUTH_IN_DEV=false

# 效能優化
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
CACHE_MAX_AGE=86400